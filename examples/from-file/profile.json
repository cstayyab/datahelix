{
  "schemaVersion": "0.1",
  "fields": [
    {
      "name": "name"
    }
  ],
  "rules": [
    {
      "rule": "notnull",
      "constraints": [
        {
          "not": {
            "field": "name",
            "is": "null"
          }
        }
      ]
    },
    {
      "rule": "name",
      "constraints": [
        {
          "field": "name",
          "is": "setFromFile",
          "value": "greek.csv"
        }
      ]
    }
  ]
}